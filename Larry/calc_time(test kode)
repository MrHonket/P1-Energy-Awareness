#include <stdlib.h>
#include <stdio.h>

int calc_time(int dato_from, int dato_to);

int main(void){


    calc_time(dato_from, dato_to);

}


int calc_time(int dato_from, int dato_to){
    int days = 0;
    int local_month = load_month

    if(year % 4 == 0){
        while(local_month >= 1){
            if(local_month == 2){
                days + 29;
                local_month - 1;
            }
            else if(local_month <= 6 && local_month % 2 == 0){
                days + 30;
                local_month - 1;
            }
            else if(local_month <= 7 && local_month % 2 != 0){
                days + 31;
                local_month - 1;
            }
            else if(local_month > 6 && local_month % 2 == 0){
                days + 31;
                local_month - 1;
            }
            else if(local_month >= 9 && local_month % 2 != 0){
                days + 30;
                local_month - 1;
            }
        }
    }
    else if (year % 4 != 0){
        while(local_month >= 1){
            if(local_month == 2){
                days + 28;
                local_month - 1;
            }
            else if(local_month <= 6 && local_month % 2 == 0){
                days + 30;
                local_month - 1;
            }
            else if(local_month <= 7 && local_month % 2 != 0){
                days + 31;
                local_month - 1;
            }
            else if(local_month > 6 && local_month % 2 == 0){
                days + 31;
                local_month - 1;
            }
            else if(local_month >= 9 && local_month % 2 != 0){
                days + 30;
                local_month - 1;
            }
        }
    }

    }

}
